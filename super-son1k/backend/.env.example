# Super-Son1k Backend Environment Variables

# Qwen API Configuration
QWEN_API_KEY=your_qwen_api_key_here
QWEN_BASE_URL=https://api.qwen.com/v1

# Server Configuration
PORT=3001
NODE_ENV=development

# Security
REQUIRE_AUTH=false
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:4173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FORMAT=combined

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:5173
CORS_CREDENTIALS=true

# Database (if needed in future)
# DATABASE_URL=postgresql://user:password@localhost:5432/super_son1k

# Redis (if needed for caching)
# REDIS_URL=redis://localhost:6379

# Monitoring
# SENTRY_DSN=your_sentry_dsn_here
# NEW_RELIC_LICENSE_KEY=your_newrelic_key_here